		"eslint-config-prettier": "^8.9.0",
		"eslint-plugin-svelte": "^2.32.4",
		"marked": "^5.1.2",
		"postcss": "^8.4.27",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.3",
		"rollup-plugin-visualizer": "^5.9.2",
		"satori": "^0.10.1",
		"satori-html": "^0.3.2",
		"svelte": "^4.1.1",
		"svelte-check": "^3.4.6",
		"svelte-local-storage-store": "^0.5.0",
		"svelte-preprocess": "^5.0.4",
		"sveltekit-search-params": "^1.0.14",
