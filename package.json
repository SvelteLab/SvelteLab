{
	"name": "sveltelab",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "pnpm run partytown && vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dependency-report": "npx --yes license-report@latest --output=json > ./src/lib/dependency-report.json",
		"dev": "vite dev",
		"format": "prettier --plugin-search-dir . --write .",
		"postinstall": "svelte-kit sync && pnpm dependency-report",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"preview": "vite preview",
		"test": "playwright test",
		"test:unit": "vitest",
		"partytown": "partytown copylib static/~partytown",
		"skip-build": "node ./skip-build/index.js"
	},
	"dependencies": {
		"@builder.io/partytown": "^0.8.0",
		"@codemirror/autocomplete": "^6.6.1",
		"@codemirror/commands": "^6.2.4",
