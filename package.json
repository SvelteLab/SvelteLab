		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"marked": "^4.3.0",
		"postcss": ">=8.4.6 <9.0.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"rollup-plugin-visualizer": "^5.9.0",
		"satori": "^0.7.3",
		"satori-html": "^0.3.2",
		"svelte": "^3.58.0",
