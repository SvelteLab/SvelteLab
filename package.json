		"@typescript-eslint/parser": "^8.0.0",
		"@zerodevx/svelte-toast": "^0.9.5",
		"eslint": "^9.6.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.41.0",
		"marked": "^14.0.0",
		"postcss": "^8.4.39",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.5",
		"rollup-plugin-visualizer": "^5.12.0",
		"satori": "^0.10.14",
		"satori-html": "^0.3.2",
		"svelte": "5.0.0-next.184",
