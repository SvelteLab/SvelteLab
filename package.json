{
	"name": "sveltelab",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "pnpm run partytown && vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dependency-report": "npx --yes license-report@latest --output=json > ./src/lib/dependency-report.json",
		"dev": "vite dev",
		"pocketbase": "./pocketbase serve",
		"format": "prettier --plugin-search-dir . --write .",
		"postinstall": "svelte-kit sync && pnpm dependency-report",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"preview": "vite preview",
		"test": "playwright test",
		"test:unit": "vitest",
		"partytown": "partytown copylib static/~partytown",
		"skip-build": "node ./skip-build/index.js"
	},
	"dependencies": {
		"@builder.io/partytown": "^0.8.0",
		"@codemirror/autocomplete": "^6.9.0",
		"@codemirror/commands": "^6.2.4",
		"@codemirror/lang-css": "^6.2.0",
		"@codemirror/lang-html": "^6.4.5",
		"@codemirror/lang-javascript": "^6.1.9",
		"@codemirror/lang-json": "^6.0.1",
		"@codemirror/lang-markdown": "^6.2.0",
		"@codemirror/language": "^6.8.0",
		"@codemirror/lint": "^6.4.0",
		"@codemirror/search": "^6.5.0",
		"@codemirror/state": "^6.2.1",
		"@codemirror/view": "^6.15.3",
		"@emmetio/codemirror6-plugin": "^0.3.0",
		"@floating-ui/dom": "^1.2.9",
		"@jridgewell/trace-mapping": "^0.3.18",
		"@lezer/common": "^1.0.3",
		"@lezer/highlight": "^1.1.6",
		"@lezer/javascript": "^1.4.5",
		"@lezer/lr": "^1.3.9",
		"@neocodemirror/svelte": "^0.0.17",
		"@replit/codemirror-lang-svelte": "^6.0.0",
		"@replit/codemirror-vim": "^6.0.14",
		"@sveltejs/site-kit": "^3.2.2",
		"@webcontainer/api": "^1.1.5",
		"@yaireo/relative-time": "^1.0.4",
		"as-comps": "^0.0.27",
		"flexsearch": "^0.7.31",
		"fzf": "^0.5.2",
		"he": "^1.2.0",
		"jszip": "^3.10.1",
		"lz-string": "^1.5.0",
		"magic-string": "^0.30.2",
		"material-icon-theme": "^4.29.0",
		"pocketbase": "^0.15.3",
		"svelte-splitpanes": "^0.8.0",
		"tinykeys": "^2.1.0",
		"uuid": "^9.0.0",
		"xterm": "^5.2.1",
		"xterm-addon-fit": "^0.7.0",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.95",
		"@playwright/test": "^1.36.2",
		"@resvg/resvg-js": "^2.4.1",
		"@sveltejs/adapter-vercel": "^3.0.2",
		"@sveltejs/kit": "^1.22.3",
